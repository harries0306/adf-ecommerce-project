{
	"name": "pl_onprem_to_adls_full_load",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ds_migration_src",
						"type": "DatasetReference",
						"parameters": {
							"p_table_name": {
								"value": "@pipeline().parameters.p1_table_name",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "ds_migration_sink",
						"type": "DatasetReference",
						"parameters": {
							"p_folder_path": {
								"value": "raw/olist_products_dataset/load_date=@{formatDateTime(utcNow(),'yyyy-MM-dd')}\n",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"p1_table_name": {
				"type": "string"
			}
		},
		"variables": {
			"p1_table_name": {
				"type": "String"
			}
		},
		"annotations": []
	}
}